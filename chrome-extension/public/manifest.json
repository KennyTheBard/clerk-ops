{
  "name": "ClerkOps",
  "description": "This extension is made for demonstration purposes",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "activeTab", "scripting"],
  "background": {
    "serviceWorker": "serviceWorker.js"
  },
  "action": {
    "default_title": "Click to view a popup",
    "default_popup": "popup.html"
  }
}
